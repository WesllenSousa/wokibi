	 <TextView 
	      android:id="@+id/listaContatosConectados_tv_nome"
	      
		  android:layout_centerVertical="true" android:layout_marginLeft="1sp"
	      android:layout_width="fill_parent" android:layout_height="wrap_content" 
	      
	      style="@style/estilo_textview_lista" android:background="@color/transparente"  />
	 
	 <TextView 
	      android:id="@+id/listaContatosConectados_tv_ip"
	      
	      android:layout_centerVertical="true" android:layout_alignParentRight="true"
	      android:layout_width="wrap_content" android:layout_height="wrap_content" 
	     
